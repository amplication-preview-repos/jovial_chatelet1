datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ApkInfo {
  applicationLabel   String?
  createdAt          DateTime @default(now())
  icon               Json?
  id                 String   @id @default(cuid())
  launchableActivity String?
  packageName        String?
  permissions        String?
  sdkVersion         Int?
  targetSdkVersion   Int?
  updatedAt          DateTime @updatedAt
  versionCode        Int?
  versionName        String?
}
